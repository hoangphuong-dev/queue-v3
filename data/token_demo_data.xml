<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- Token cho Lê Thị Hồ -->
        <record id="token_le_thi_ho_1" model="queue.token">
            <field name="patient_id" ref="patient_le_thi_ho"/>
            <field name="service_id" ref="service_blood_test"/>
            <field name="state">completed</field>
            <field name="priority">4</field>
        </record>
        
        <record id="token_le_thi_ho_2" model="queue.token">
            <field name="patient_id" ref="patient_le_thi_ho"/>
            <field name="service_id" ref="service_xray"/>
            <field name="state">waiting</field>
            <field name="priority">4</field>
        </record>

        <!-- Token cho Nguyễn Vũ Nam -->
        <record id="token_nguyen_vu_nam_1" model="queue.token">
            <field name="patient_id" ref="patient_nguyen_vu_nam"/>
            <field name="service_id" ref="service_blood_test"/>
            <field name="state">completed</field>
        </record>

        <!-- Thêm token cho các bệnh nhân khác tương tự... -->
    </data>
</odoo>